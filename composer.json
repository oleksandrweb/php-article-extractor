{
  "name": "crscheid/php-article-extractor",
  "description": "An HTML article extractor for PHP",
  "keywords": [
    "parse",
    "HTML",
    "extract"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Christopher Scheidel",
      "email": "christopher.scheidel@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "scotteh/php-goose": "dev-master",
    "paquettg/php-html-parser": "^3.1.1",
    "detectlanguage/detectlanguage": "2.*",
    "andreskrey/readability.php": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
